<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>dev.pow.powd</string>

    <key>ProgramArguments</key>
    <array>
      <string>/opt/boxen/homebrew/opt/node/bin/node</string>
      <string>/opt/boxen/homebrew/opt/pow/libexec/bin/pow</string>
    </array>

    <key>KeepAlive</key>
    <true/>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/opt/boxen/log/pow.log</string>

    <key>StandardOutPath</key>
    <string>/opt/boxen/log/pow.log</string>
  </dict>
</plist>
